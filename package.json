{
  "name": "cxperium-bot-sample",
  "version": "1.0.0",
  "description": "Cxperium Bot Sample",
  "author": "QSoft Bili≈üim",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "set:engine:include": "node ./engine-include.js yes",
    "set:engine:exclude": "node ./engine-include.js no",
    "dev": "npm run set:engine:exclude && NODE_ENV=development ts-node ./src",
    "start": "npm run set:engine:exclude && NODE_ENV=production node ./dist",
    "dev:engine": "npm run set:engine:include && NODE_ENV=development ts-node ./src",
    "start:engine": "npm run set:engine:include && NODE_ENV=production node ./dist",
    "dev:build:engine": "cd ./engine && npm run build && cd .. && npm run dev:engine ",
    "start:build:engine": "cd ./engine && npm run build && cd .. && npm run start:engine ",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qsoft-git/cxperium-bot-sample.git"
  },
  "bugs": {
    "url": "https://github.com/qsoft-git/cxperium-bot-sample/issues"
  },
  "homepage": "https://github.com/qsoft-git/cxperium-bot-sample#readme",
  "dependencies": {
    "cxperium-bot-engine": "^1.0.18",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-hooks": "^1.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}