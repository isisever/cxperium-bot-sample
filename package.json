{
  "name": "cxperium-bot-sample",
  "version": "1.0.0",
  "description": "Cxperium Bot Sample",
  "author": "QSoft Bili≈üim",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "set:engine:include": "node ./engine-include.js yes",
    "set:engine:exclude": "node ./engine-include.js no",
    "dev": "NODE_ENV=development ts-node ./src",
    "start": "NODE_ENV=production node ./dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qsoft-git/cxperium-bot-sample.git"
  },
  "bugs": {
    "url": "https://github.com/qsoft-git/cxperium-bot-sample/issues"
  },
  "homepage": "https://github.com/qsoft-git/cxperium-bot-sample#readme",
  "dependencies": {
    "cxperium-bot-engine": "file:./engine",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsc-hooks": "^1.1.2",
    "typescript": "^5.3.3"
  }
}
